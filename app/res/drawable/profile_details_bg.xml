<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- "border" -->
    <item>
        <shape android:shape="rectangle" >
            <solid android:color="@color/profile_icon_color_transparent" />
        </shape>
    </item>
    <!-- background color -->
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle" >
            <solid android:color="#fff" />
        </shape>
    </item>

</layer-list>