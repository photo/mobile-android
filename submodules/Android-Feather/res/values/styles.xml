    </style>

    <style name="FeatherOptionPanel.EffectThumbGetMore.MainImage">
        <item name="android:layout_marginLeft">@dimen/feather_effects_getmore_image_margin_left</item>
        <item name="android:layout_marginRight">@dimen/feather_effects_getmore_image_margin_right</item>
        <item name="android:layout_marginTop">@dimen/feather_effects_getmore_image_margin_top</item>
        <item name="android:background">@drawable/feather_getmore_image_bg</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:src">@drawable/feather_more_button_selector</item>
    </style>

    <style name="FeatherOptionPanel.EffectThumbGetMore.MainText" parent="FeatherOptionPanel.ThumbText">
        <item name="android:layout_marginRight">@dimen/feather_effects_getmore_text_margin_right</item>
    </style>

    <style name="FeatherOptionPanel.EffectThumbGetMore.RibbonLayout">
        <item name="android:background">@drawable/feather_ribbon_bg</item>
        <item name="android:layout_marginRight">@dimen/feather_effects_getmore_bubble_margin_right</item>
    </style>

    <style name="FeatherOptionPanel.EffectThumbGetMore.MainLayout.Inverted">
        <item name="android:background">@drawable/feather_getmore_layout_bg_inverted</item>
    </style>

    <style name="FeatherOptionPanel.EffectThumbGetMore.MainImage.Inverted">
        <item name="android:layout_marginLeft">@dimen/feather_effects_getmore_image_margin_right</item>
        <item name="android:layout_marginRight">@dimen/feather_effects_getmore_image_margin_left</item>
        <item name="android:layout_marginTop">@dimen/feather_effects_getmore_image_margin_top</item>
        <item name="android:background">@drawable/feather_getmore_image_bg</item>
    </style>

    <style name="FeatherOptionPanel.EffectThumbGetMore.MainText.Inverted">
        <item name="android:layout_marginRight">0dip</item>
        <item name="android:layout_marginLeft">@dimen/feather_effects_getmore_text_margin_right</item>
    </style>

    <style name="FeatherOptionPanel.EffectThumbGetMore.RibbonText" parent="FeatherOptionPanel.ThumbText">
        <item name="android:textSize">?attr/textSizeMedium</item>
        <item name="android:paddingBottom">@dimen/feather_effects_getmore_bubble_text_padding_bottom</item>
        <item name="android:paddingTop">0dip</item>
    </style>

    <style name="FeatherOptionPanel.EffectThumbGetMore.RibbonLayout.Inverted">
        <item name="android:layout_marginLeft">@dimen/feather_effects_getmore_bubble_margin_right</item>
        <item name="android:layout_marginRight">0dip</item>
    </style>

    <style name="FeatherOptionPanel.EffectThumbGetMore.RibbonText.Inverted"></style>

    <style name="FeatherOptionPanel.BorderTopBackground">
        <item name="android:layout_height">@dimen/feather_borders_panel_top_bg_height</item>
    </style>

    <style name="FeatherOptionPanel.Background">
        <item name="android:paddingTop">0dp</item>
        <item name="android:background">?attr/optionPanelBackground</item>
    </style>

    <style name="FeatherOptionPanel.Background2">
        <item name="android:paddingTop">0dp</item>
        <item name="android:background">?attr/optionPanelBackground2</item>
    </style>

    <style name="FeatherOptionPanel.TopLine">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/feather_panel_top_line_height</item>
        <item name="android:background">@color/feather_panel_top_line_color</item>
    </style>

    <style name="FeatherOptionPanel.BottomLine">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/feather_panel_bottom_line_height</item>
        <item name="android:background">@color/feather_panel_bottom_line_color</item>
    </style>

    <style name="FeatherOptionPanelWheel">
        <item name="android:padding">0dp</item>
    </style>

    <style name="FeatherOptionPanelWheel.Background">
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingLeft">3dp</item>
        <item name="android:paddingRight">3dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:background">@drawable/feather_toolbar_background</item>
    </style>

    <style name="FeatherOptionPanelImageButton">
        <item name="android:paddingBottom">16dp</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_margin">0dp</item>
        <item name="android:background">@drawable/feather_default_button_background</item>
    </style>

    <style name="FeatherOptionPanelImageButtons">
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_margin">0dp</item>
    </style>

    <style name="FeatherOptionPanelImageButtons.Left">
        <item name="android:background">@drawable/feather_left_button_background</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
    </style>

    <style name="FeatherOptionPanelImageButtons.Right">
        <item name="android:background">@drawable/feather_right_button_background</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
    </style>

    <style name="FeatherOptionPanelImageButtons.Center">
        <item name="android:background">@drawable/feather_center_button_background</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
    </style>

    <style name="FeatherOptionPanelButton" parent="@style/FeatherOptionPanelImageButton">
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">2.0</item>
        <item name="android:shadowRadius">2.0</item>
        <item name="android:shadowColor">@android:color/black</item>
        <item name="font">?attr/fontFamilyLight</item>
        <item name="android:textAppearance">?attr/textAppearanceSmall</item>
        <item name="android:textColor">@color/feather_option_panel_text_selector</item>
    </style>

    <style name="FeatherOptionPanelMemeButton">
        <item name="android:background">@drawable/feather_meme_button_bg</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:shadowDx">?attr/shadowDxMedium</item>
        <item name="android:shadowDy">?attr/shadowDyMedium</item>
        <item name="android:shadowRadius">?attr/shadowRadiusMedium</item>
        <item name="android:shadowColor">?attr/shadowColorPrimaryInverse</item>
        <item name="android:ellipsize">end</item>
        <item name="font">?attr/fontFamilyMedium</item>
        <item name="android:paddingTop">10dip</item>
        <item name="android:paddingBottom">10dip</item>
        <item name="android:paddingRight">26dip</item>
        <item name="android:textAppearance">?attr/textAppearanceSmall</item>
        <item name="android:textColor">@color/feather_option_panel_text_selector</item>
    </style>

    <style name="FeatherOptionPanelTextButton">
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:shadowDx">?attr/shadowDxMedium</item>
        <item name="android:shadowDy">?attr/shadowDyMedium</item>
        <item name="android:shadowRadius">?attr/shadowRadiusMedium</item>
        <item name="android:shadowColor">?attr/shadowColorPrimaryInverse</item>
        <item name="android:focusable">true</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="font">?attr/fontFamilyLight</item>
        <item name="android:textAppearance">?attr/textAppearanceSmall</item>
    </style>

    <style name="FeatherHiddenEditText" parent="@android:style/Widget.EditText">
        <item name="android:background">@drawable/feather_hidden_edit_text</item>
        <item name="android:textColor">@android:color/transparent</item>
        <item name="android:editable">true</item>
        <item name="android:imeOptions">actionDone</item>
        <item name="android:inputType">textNoSuggestions</item>
        <item name="android:maxHeight">1sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">1sp</item>
    </style>

    <style name="FeatherInlineProgressText">
        <item name="font">?attr/fontFamilyLight</item>
        <item name="android:textAppearance">?attr/textAppearanceSmall</item>
        <item name="android:textColor">@color/feather_inline_loading_color</item>
    </style>

    <style name="FeatherWheel">
        <item name="ticks">24</item>
        <item name="numRotations">4</item>
    </style>

    <style name="FeatherWheelRadio">
        <item name="smallTicks">28</item>
        <item name="bigTicks">5</item>
    </style>

    <!-- stickers -->
